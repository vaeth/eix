msgstr ""

#: src/eixrc/defaults.cc:1070
msgctxt "REDUNDANT_IF_IN_ENV"
msgid "Applies if /etc/portage/package.env matches."
msgstr ""

#: src/eixrc/defaults.cc:1074
msgctxt "REDUNDANT_IF_IN_LICENSE"
